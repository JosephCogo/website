<div class="actions">
        </div>

        <div class="question-stack">
            {{#each question in controller}}
                
                    {{#view App.QuestionView contentBinding="this"}}
                         <p class = "questionParagraph"> {{question.qbody}} </p>
                    {{/view}}
                    
                    {{else}}
                         <span>There are no questions to answer</span>
            {{/each}}
            
            {{outlet}}

        </div>
